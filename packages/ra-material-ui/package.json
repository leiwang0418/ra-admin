{
  "name": "ra-material-ui",
  "version": "0.0.1",
  "description": "ui components with material-ui",
  "author": "lei <lei.wang0418@gmail.com>",
  "homepage": "https://github.com/leiwang0418/ra-admin.git",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "styleguide": "yarn lang && styleguidist server",
    "build": "yarn lang && styleguidist build",
    "test": "yarn lang && react-scripts test",
    "lang": "yarn extract && yarn compile",
    "extract": "formatjs extract \"src/**/*.tsx\" --out-file src/lang/en.json",
    "compile": "formatjs compile src/lang/en.json --out-file src/compiled-lang/en.json"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/core": "next",
    "@mui/icons-material": "next",
    "@mui/material": "next",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.5",
    "react-is": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-styleguidist": "^11.1.7",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.28",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/styled-components": "^5.1.12"
  }
}
